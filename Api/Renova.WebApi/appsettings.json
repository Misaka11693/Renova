{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "App": {
    "SelfUrl": "https://localhost:1647", //可访问https://127.0.0.1:1033/index.html（换1031不行，可能1031端口被占）
    "AssemblyPrefixes": [ "MyApp." ]
  },

  // 加载自定义json配置文件
  "JsonConfiguration": {
    "Enabled": true,
    "Folder": "Config", // 需要加载的目录名
    "Include": [ "*.json" ], // 包含文件
    "Exclude": [] // 排除文件
  }
}
